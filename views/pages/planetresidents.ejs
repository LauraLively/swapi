<!DOCTYPE html>
<html lang="en">
<head>
    <title>Star Wars API</title>
    <% include ../partials/head %>
</head>
<body class="container">
    <header>
        <% include ../partials/header %>
    </header>
    <main>
        <div class="jumbotron">
            <h1>Star Wars Planet Residents</h1>
            <table>
                <% data.forEach(function(planet) { %>
              <tr>
                <td><%= planet.name %></td>
                <td><%= planet.residents %></td>
              </tr>
              <% }); %>

            </table>
        </div>
    </main>
    <footer>
        <% include ../partials/footer %>
    </footer>
</body>
</html>