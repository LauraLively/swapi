<!DOCTYPE html>
<html lang="en">
<head>
    <title>Star Wars API</title>
    <% include ../partials/head %>
</head>
<body class="container">
    <header>
        <% include ../partials/header %>
    </header>
    <main>
        <div class="jumbotron">
            <h1 style="font-size: x-large">Star Wars Planet Residents</h1>
            <table class="table">
                <% data.forEach(function(planet) { %>
              <tr>
                <td><%= planet.name %></td>
                <td ><%= planet.residents %> </td>
              </tr>
              <% }); %>

            </table>
            <% data %>
        </div>
    </main>
    <footer>
        <% include ../partials/footer %>
    </footer>
</body>
</html>